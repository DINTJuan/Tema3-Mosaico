<Window x:Class="Tema3_Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema3_Mosaico"
        mc:Ignorable="d"
        Title="Mosaico" Height="450" Width="800" MinHeight="400" MinWidth="400" Icon="Imagenes\128 (4).ico">
    <Grid>
        <DockPanel>
            <TextBlock Height="100" Background="LightGreen"  DockPanel.Dock="Top"/>
            <TextBlock Width="100" Background="BlanchedAlmond" DockPanel.Dock="Left"/>
            <TextBlock Height="100" Background="Aqua" DockPanel.Dock="Bottom"/>
            <TextBlock Width="100" Background="DeepPink"  DockPanel.Dock="Right"/>
            <TextBlock Height="100" Background="GreenYellow"  DockPanel.Dock="Bottom"/>
            <TextBlock Width="100" Background="GreenYellow" DockPanel.Dock="Left"/>
            <TextBlock Height="100" Background="DarkViolet" DockPanel.Dock="Top"/>
            <TextBlock Width="100" Background="DarkViolet" DockPanel.Dock="Right"/>
            <TextBlock Background="Black" DockPanel.Dock="Top"/>
        </DockPanel>
    </Grid>
</Window>
